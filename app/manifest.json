{
    "name": "__MSG_appName__",
    "description": "__MSG_appDescription__",
    "short_name": "__MSG_shortName__",
    "version": "1.7.11",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        "fileSystemProvider",
        "storage",
        "notifications",
        {
            "socket": [
                "resolve-host",
                "tcp-connect:*:*"
            ]
        }
    ],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "bower_components/cryptojslib/components/core-min.js",
                "bower_components/cryptojslib/components/enc-base64-min.js",
                "bower_components/cryptojslib/components/lib-typedarrays-min.js",
                "scripts/task_queue.js",
                "scripts/metadata_cache.js",
                "scripts/sftp_fs.js",
                "scripts/sftp_client.js",
                "scripts/background.js",
                "scripts/chromereload.js"
            ]
        }
    },
    "file_system_provider_capabilities": {
        "source": "network",
        "multiple_mounts": true
    },
    "minimum_chrome_version": "42"
}
